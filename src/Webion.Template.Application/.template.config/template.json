{
    "$schema": "http://json.schemastore.org/template",
    "author": "Webion",
    "classifications": [ "Web", "ASP" ],
    "identity": "Webion.Template.Application",
    "name": "Webion Application project",
    "shortName": "webion-application-proj",
    "sourceName": "Webion.Template.Application",
    "tags": {
      "language": "C#",
      "type": "solution"
    },
    "symbols": {
        "ProjectName": {
            "type": "parameter",
            "isRequired": "true",
            "description": "Short PascalCase name for the project, used where it's appended to class names",
            "datatype": "text",
            "replaces": "{ProjectName}"
        }
    },
    "preferNameDirectory": true
}